<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:System="clr-namespace:System;assembly=mscorlib">
    <Color x:Key="SNForegroundColour">#FF000000</Color>
    <Color x:Key="SNBackgroundColour">#FF33B5E5</Color>
    <Color x:Key="SNTranslucentWhileColour">#66EEEEEE</Color>
    <SolidColorBrush x:Key="SNBackgroundBrush" Color="{StaticResource SNBackgroundColour}"/>
    <SolidColorBrush x:Key="SNNoteBackgroundBrush" Color="White"></SolidColorBrush>
    <SolidColorBrush x:Key="SNTranslucentBackgroundBrush" Color="{StaticResource SNTranslucentWhileColour}"></SolidColorBrush>
</ResourceDictionary>